<%= simple_form_for [@dog] do |f| %>
  <%#= f.input :rating, input_html: { min: 0, max: 5 } %>
  <%= f.input :name, as: :string %>
  <%= f.input :breed, as: :string %>
  <%= f.input :age, as: :integer %>
  <%= f.input :size, as: :string %>
  <%= f.input :price, as: :integer %>
  <%= f.input :photo, as: :file %>
  <%= f.submit class: 'btn btn-primary mt-2' %>
<% end %>
